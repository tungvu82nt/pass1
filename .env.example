# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Project URL
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous Key (safe to expose in frontend)
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Sample Data Configuration (for development/testing)
VITE_ENABLE_SAMPLE_DATA=true
VITE_SAMPLE_PASSWORDS='[{"service":"Gmail","username":"user@gmail.com","password":"sample_password_123"},{"service":"Facebook","username":"user@facebook.com","password":"fb_password_456"},{"service":"GitHub","username":"developer","password":"github_token_789"}]'

# Logging Configuration
VITE_LOG_LEVEL=debug

# Note: Never commit .env.local to version control
# Add .env.local to your .gitignore file